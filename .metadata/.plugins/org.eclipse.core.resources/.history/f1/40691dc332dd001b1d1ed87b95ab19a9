
public class Test {

	public static void main(String[] args) {
		
		
		int sum = 0;
		int score[] = new int[10];
		score[0] = 1;
		score[1] = 2;
		score[2] = 3;
		score[3] = 4;
		score[4] = 5;
		score[5] = 6;
		score[6] = 7;
		score[7] = 8;
		score[8] = 9;
		score[9] = 10;
		
		for(int i = 0; 0 < score.length; i++) {
			sum += score[i];
		}
		System.out.println(sum);
	}

}
