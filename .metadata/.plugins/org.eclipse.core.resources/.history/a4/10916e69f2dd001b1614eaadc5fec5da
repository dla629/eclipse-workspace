package inheritance;

public class CustomerTest1 {

	public static void main(String[] args) {
		
		Customer ctlee = new Customer();
		ctlee.customerID = 10010;
		ctlee.customerName = "이순신";
		ctlee.bonusPoint = 1000;
		ctlee.showCustomerInfo();
		
		VIPCustomer ctkim = new VIPCustomer();
		ctkim.customerID = 10020;
		ctkim.customerName = "김유신";
		ctkim.bonusPoint = 10000;
		ctkim.showCustomerInfo();
		

	}

}
