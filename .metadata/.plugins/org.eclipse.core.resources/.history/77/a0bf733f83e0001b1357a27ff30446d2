package test;

public abstract class Car {
	public void drive();{
		System.out.println("달립니다.");
	}
	public void stop();
	public void washCar();
	
	
	public void start() {
		System.out.println("시동을 켭니다.");
	}
	
	public void turnOff() {
		System.out.println("시동을 끕니다.");
	}

	final public void run() {
		start();
		drive();
		stop();
		turnOff();
		washCar();
	}
}
